{
  "name": "taskflow",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "prisma migrate dev --schema=./prisma/schema",
    "db:migrate": "prisma migrate deploy --schema=./prisma/schema",
    "db:push": "prisma db push --schema=./prisma/schema",
    "db:studio": "prisma studio --schema=./prisma/schema",
    "db:reset": "prisma migrate reset --schema=./prisma/schema",
    "dev": "next dev --turbo",
    "dev-https": "next dev --experimental-https",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate --schema=./prisma/schema",
    "fix": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache && next lint --fix",
    "lint": "next lint --dir src",
    "lint:fix": "next lint --fix --dir src",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "generate-icons": "node scripts/generate-icons.js",
    "generate-screenshots": "node scripts/generate-screenshots.js",
    "registry:build": "shadcn build"
